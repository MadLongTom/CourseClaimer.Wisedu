{
  "BasePath": "https://jwxk.hrbeu.edu.cn/",
  "AuthPath": "https://jwxk.hrbeu.edu.cn/xsxk/auth/login",
  "DBProvider": "SQLite",
  "DBProvider_CAP": "InMemory",
  "ReLoginDelayMilliseconds": 300000,
  "CapTakeNum": 5,
  "QuartzDelayMilliseconds": 300000,
  "CronSchedule": "0 4/5 7-21 1,26,27,28,29,30,31 * ? ",
  "CronScheduleEnd": "0 0 22 * * ? ",
  "UseQuartz": true,
  "LegacyMode": false,
  "GlobalExceptionList": "党史专题讲座（网络）",
  "LimitListMillSeconds": 400,
  "LimitAddMillSeconds": 350,

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "System.Net": "Warning",
      "DotNetCore": "Warning",
      "Polly": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BootstrapBlazorOptions": {
    "ToastDelay": 4000,
    "MessageDelay": 4000,
    "SwalDelay": 4000,
    "EnableErrorLogger": false,
    "FallbackCulture": "en",
    "SupportedCultures": [
      "zh-CN",
      "en-US"
    ],
    "TableSettings": {
      "CheckboxColumnWidth": 36
    },
    "IgnoreLocalizerMissing": true,
    "StepSettings": {
      "Short": "1",
      "Int": "1",
      "Long": "1",
      "Float": "0.1",
      "Double": "0.01",
      "Decimal": "0.01"
    },
    "DefaultCultureInfo": "zh-CN"
  }
}
